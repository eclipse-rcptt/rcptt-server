Ensure that injection configuration is propagated from Maven plugin to agent.
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

let [val info [invoke-static -pluginId "org.eclipse.ui.workbench" -className "org.eclipse.ui.internal.ConfigurationInfo" -methodName "getSystemSummary"] ] {
	trace $info
	$info | contains "org.eclipse.core.runtime" | verify-true
	$info | contains "org.eclipse.nebula.widgets.opal" | verify-false
	$info | contains "org.eclipse.nebula.widgets.grid" | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
