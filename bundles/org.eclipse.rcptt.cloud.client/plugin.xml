<plugin>
   <extension
         id="headless"
         point="org.eclipse.core.runtime.applications">
      <application
            visible="true">
         <run
               class="org.eclipse.rcptt.cloud.client.ClientApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.rcptt.client.headless"
            name="Q7">
         <property
               name="appName"
               value="Q7 Cloud client">
         </property>
      </product>
   </extension>
</plugin>
